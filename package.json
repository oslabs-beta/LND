	"type": "module",
	"scripts": {
		"clean": "rm -rf ./node_modules yarn.lock && yarn run watch",
		"build:check": "tsc -watch .",
		"build:server": "tsc -p ./tsconfig.server.json",
		"start:server": "ts-node-dev -p --respawn --inspect --transpile-only ./src/server/server.ts",
		"start": "concurrently \"yarn run start:server\" \"yarn run dev\"",
		"build:dev": "webpack --mode=development",
		"build:prod": "webpack --mode=production --node-env=production",
		"watch": "webpack --watch",
		"test": "jest",
		"dev": "webpack serve --open",
		"build:backend": "tsc"
	},
